alias snk='setup-notebook-keyboard.sh'

alias finsesh='cd ~/Dropbox/fini && vim -S finsesh'
alias cdp='cd ~/Projects'
alias gs='git status'
alias gb='git branch'
alias ltr='ls -ltra'

alias t="task"
alias twcl='t context lateral'
alias twcn='t context none'
alias twcg='t context ground'
alias twch='t context horizon'
alias twch='t context out'

alias twcll='t context lateral; t'
alias twcnl='t context none; t'
alias twcgl='t context ground; t'
alias twchl='t context horizon; t'
alias twcol='t context out; t'
alias twan='t context none; t +ACTIVE or +next'

alias twa="t add"
alias twal="t add +lateral"
alias twag="t add +ground"
alias twah="t add +horizon"
alias twao="t add +out"
